<?xml version="1.0" encoding="utf-8"?>
<changelogs extension="{{name}}"
	description="{{description}}"
	projecturl="{{projecturl}}"
	maintainerurl="{{maintainerurl}}"
	maintainer="{{maintainer}}"
	infosDE="{{infosDE}}"
	infosEN="{{infosEN}}"
	docsDE="{{docsDE}}"
	docsEN="{{docsEN}}"
	bugs="{{bugs}}"
>

<!--{{nameUpper}} Version {{version}}-->
	<changelog>
		<element>{{element}}</element>
		<type>{{type}}</type>
		<folder>{{folder}}</folder>
		<client>{{client}}</client>
		<version>{{version}}</version>

		<addition>

		</addition>

		<change>
			<item><![CDATA[
				<a href="https://github.com/GHSVS-de/{{name}}/compare/{{versionCompare}}...{{version}}">
					Compare code {{versionCompare}}...{{version}}
				</a>
			]]></item>
		</change>

		<fix>

		</fix>

		<language>

		</language>

		<note>
			<item><![CDATA[
				Last tests: {{lastTests}}
			]]></item>

			<item><![CDATA[
				<a href="https://github.com/GHSVS-de/{{name}}/raw/{{version}}/dist/{{zipFilename}}">
					Direct download {{zipFilename}}
				</a>
			]]></item>

			<item><![CDATA[
				<a href="https://updates.ghsvs.de/changelog.php?file={{filename}}">
					All changelogs
				</a>
			]]></item>

			<item><![CDATA[
				<a href="https://github.com/GHSVS-de/{{name}}/releases">
					All releases page
				</a>
			]]></item>

			<item><![CDATA[
				Checksum: <span style="word-wrap:anywhere;">{{checksumEntity}}</span>
			]]></item>
		</note>

		<remove>

		</remove>

		<requirement>
			<entry><![CDATA[
				minimumPhp: {{minimumPhp}}
			]]></entry>

			<entry><![CDATA[
				maximumPhp: {{maximumPhp}}
			]]></entry>

			<entry><![CDATA[
				minimumJoomla: {{minimumJoomla}}
			]]></entry>

			<entry><![CDATA[
				maximumJoomla: {{maximumJoomla}}
			]]></entry>

			<entry><![CDATA[
				allowDowngrades: {{allowDowngrades}}
			]]></entry>
		</requirement>

		<security>

		</security>

	</changelog>
<!--/{{nameUpper}} Version {{version}}-->

</changelogs>
